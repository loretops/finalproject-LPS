{
  "name": "coopco",
  "version": "0.1.0",
  "description": "Plataforma exclusiva para club privado de inversores inmobiliarios",
  "main": "backend/server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cross-port-killer -p $FRONTEND_PORT && cd frontend && next dev -p $FRONTEND_PORT",
    "dev:backend": "cross-port-killer -p $BACKEND_PORT && cd backend && nodemon server.js",
    "build": "cd frontend && npm run build",
    "start": "node backend/server.js",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "express": "^4.18.2",
    "next": "^14.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.1",
    "prisma": "^5.5.2",
    "@prisma/client": "^5.5.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2",
    "cross-port-killer": "^1.4.0",
    "jest": "^29.7.0",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.33.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "private": true
} 